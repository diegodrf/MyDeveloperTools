@page "/"
@inject NavigationManager _navigationManager


<MudGrid>
    @foreach (var route in RouteConstant.GetAllRoutes())
    {
        <MudItem xs=5>
            <MudCard>
                <MudCardContent>
                    <MudText Typo="Typo.h6">@(route.GetPathTitle())</MudText>
                    </MudCardContent>
                    <MudCardActions>
                        <MudButton Variant="Variant.Text" Color="Color.Primary" OnClick="() => _navigationManager.NavigateTo(route)">Go To</MudButton>
                    </MudCardActions>
                </MudCard>
            </MudItem>
    }
</MudGrid>

