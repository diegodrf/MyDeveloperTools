@page "/"
@inject NavigationManager _navigationManager


<MudGrid>
    @foreach (var route in RouteConstant.GetAllRoutes())
    {
        <MudItem xs=6>
            <MudButton 
                Variant="Variant.Filled"
                OnClick="() => _navigationManager.NavigateTo(route)"
                Color="Color.Primary">
                <MudText Typo="Typo.h6">@(route.GetPathTitle())</MudText>
            </MudButton>
        </MudItem>
    }
</MudGrid>

